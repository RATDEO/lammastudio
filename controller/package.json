{
  "name": "vllm-studio-controller",
  "version": "0.3.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/main.ts",
    "start": "bun src/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "script:util:compare": "bun scripts/utilities/compare-controllers.ts",
    "prepare": "cd ../.. && husky controller/.husky",
    "test": "vitest run",
    "check": "knip && jscpd src && depcheck",
    "check:fix": "knip --fix"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.3",
    "@hono/zod-openapi": "1.2.0",
    "dotenv": "16.6.1",
    "hono": "4.6.12",
    "pg": "8.13.3",
    "prom-client": "15.1.3",
    "swagger-ui-dist": "^5.18.0",
    "yaml": "2.8.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "bun-types": "1.3.6",
    "@types/node": "24.6.0",
    "@types/pg": "8.16.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "eslint": "9.35.0",
    "eslint-plugin-jsdoc": "59.1.0",
    "eslint-plugin-unicorn": "60.0.0",
    "husky": "9.1.7",
    "jscpd": "4.0.5",
    "knip": "5.44.2",
    "lint-staged": "15.2.11",
    "depcheck": "1.4.7",
    "prettier": "3.4.2",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  }
}
